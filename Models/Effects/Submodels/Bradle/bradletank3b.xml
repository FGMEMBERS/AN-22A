<PropertyList>

  <path>Bradle.ac</path>
    
  <animation>
    <name>bradletank3b</name>    
  </animation>
  
  
  
  <animation>
    <type>translate</type>
    <name>bradletank3b</name>
    <property>sim/multiplay/generic/float[16]</property>
    <interpolation>
      <entry><ind> 182550 </ind><dep> -0.0 </dep></entry>
      <entry><ind> 170000 </ind><dep> -1.0 </dep></entry>
      <entry><ind> 164000 </ind><dep> -5.0 </dep></entry>
      <entry><ind> 145000 </ind><dep> -10.0 </dep></entry>
      <entry><ind> 132000 </ind><dep> -18.0 </dep></entry>
      <entry><ind> 121701 </ind><dep> -36.0 </dep></entry>
      <entry><ind> 0 </ind><dep> -70.0 </dep></entry>
    </interpolation>
    <axis>
      <x>-1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <name>bradletank3b</name>
    <property>sim/multiplay/generic/float[16]</property>
    <interpolation>
      <entry><ind> 145000 </ind><dep>  0.0 </dep></entry>      
      <entry><ind> 138500 </ind><dep>  20.0 </dep></entry>
      <entry><ind> 132000 </ind><dep>  0.0 </dep></entry>      
    </interpolation>
    <center>
      <x-m> 11.50 </x-m>
      <y-m>  0.0 </y-m>
      <z-m>  0.0 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

 <animation>
    <type>translate</type>
    <name>bradletank3b</name>
    <property>sim/multiplay/generic/float[16]</property>
    <interpolation>
      <entry><ind> 182550 </ind><dep>  0.8 </dep></entry>
      <entry><ind> 170000 </ind><dep>  0.8 </dep></entry>
      <entry><ind> 164000 </ind><dep>  0.7 </dep></entry>
      <entry><ind> 145000 </ind><dep>  0.6 </dep></entry>
      <entry><ind> 132000 </ind><dep>  2.0 </dep></entry>
      <entry><ind> 121701 </ind><dep>  2.0 </dep></entry>
      <entry><ind> 0 </ind><dep>  2.0 </dep></entry>
    </interpolation>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z>-1 </z>
    </axis>
  </animation>
  
</PropertyList>



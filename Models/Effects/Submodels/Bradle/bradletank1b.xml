<PropertyList>

  <path>Bradle.ac</path>
    
  <animation>
    <name>bradletank1b</name>    
  </animation>
  
  
  
  <animation>
    <type>translate</type>
    <name>bradletank1b</name>
    <property>sim/multiplay/generic/float[16]</property>
    <interpolation>
      <entry><ind> 60850 </ind><dep> 0.0 </dep></entry>
      <entry><ind> 58850 </ind><dep> -1.0 </dep></entry>
      <entry><ind> 50850 </ind><dep> -5.0 </dep></entry>
      <entry><ind> 35850 </ind><dep> -10.0 </dep></entry>
      <entry><ind> 25000 </ind><dep> -18.0 </dep></entry>
      <entry><ind> 1 </ind><dep> -50.0 </dep></entry>
      <entry><ind> 0 </ind><dep> -50.0 </dep></entry>
    </interpolation>
    <axis>
      <x>-1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <name>bradletank1b</name>
    <property>sim/multiplay/generic/float[16]</property>
    <interpolation>
      <entry><ind> 25000 </ind><dep>  0.0 </dep></entry>      
      <entry><ind> 19500 </ind><dep>  20.0 </dep></entry>
      <entry><ind> 14000 </ind><dep>  0.0 </dep></entry>      
    </interpolation>
    <center>
      <x-m> 26.50 </x-m>
      <y-m>  0.0 </y-m>
      <z-m>  0.0 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

 <animation>
    <type>translate</type>
    <name>bradletank1b</name>
    <property>sim/multiplay/generic/float[16]</property>
    <interpolation>
      <entry><ind> 60850 </ind><dep>  0.7 </dep></entry>
      <entry><ind> 50000 </ind><dep>  0.7 </dep></entry>
      <entry><ind> 35000 </ind><dep>  0.6 </dep></entry>
      <entry><ind> 25000 </ind><dep>  0.6 </dep></entry>
      <entry><ind> 14000 </ind><dep>  2.0 </dep></entry>
      <entry><ind> 1 </ind><dep>  2.0 </dep></entry>
      <entry><ind> 0 </ind><dep>  2.0 </dep></entry>
    </interpolation>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z>-1 </z>
    </axis>
  </animation>
  
</PropertyList>
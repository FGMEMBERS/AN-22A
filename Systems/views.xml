<PropertyList>

  <chase-distance-m type="double" archive="y"> -80 </chase-distance-m>

  <view n="100">
    <name>Co Pilote</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y">   0.44156 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  -0.75604 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -24.52172 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> -5 </pitch-offset-deg>
      <default-field-of-view-deg type="double"> 80 </default-field-of-view-deg>
    </config>
  </view>

  <view n="101">
    <name>Engineer</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y">   0.77 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  -1.16 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -23.34 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> -2 </pitch-offset-deg>
      <default-field-of-view-deg type="double"> 80 </default-field-of-view-deg>
      <heading-offset-deg type="double">180 </heading-offset-deg>
    </config>
  </view>
  
  <view n="102">
    <name>Ramp Control</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y">   2.23 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  -1.94 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> 0.26 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> -5 </pitch-offset-deg>
      <default-field-of-view-deg type="double"> 80 </default-field-of-view-deg>
      <heading-offset-deg type="double">180 </heading-offset-deg>
    </config>
  </view>
  
  <view n="103">
    <name>Navigator View</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y">  0.0 </x-offset-m> <!--Right-->
      <y-offset-m archive="y">  -2.82 </y-offset-m> <!--Up-->
      <z-offset-m archive="y"> -26.06 </z-offset-m> <!--Back-->
      <pitch-offset-deg> -19 </pitch-offset-deg>
      <default-field-of-view-deg type="double"> 75 </default-field-of-view-deg>
    </config>
  </view>
  
  <view n="104">
    <name>Navigator View Guest</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y">  0.71 </x-offset-m> <!--Right-->
      <y-offset-m archive="y">  -2.89 </y-offset-m> <!--Up-->
      <z-offset-m archive="y"> -26.71 </z-offset-m> <!--Back-->
      <pitch-offset-deg> -25 </pitch-offset-deg>
      <heading-offset-deg type="double">30 </heading-offset-deg>
      <default-field-of-view-deg type="double"> 120.0 </default-field-of-view-deg>
    </config>
  </view>
  
  <view n="0">
    <internal archive="y">true</internal>
    <config>
      <x-offset-m archive="y">  -0.50 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  -0.76 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -24.38 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> -5 </pitch-offset-deg>
      <default-field-of-view-deg type="double"> 95 </default-field-of-view-deg>
    </config>
  </view>

  <view n="1">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="2">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="3">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="4">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="5">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="6">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

</PropertyList>

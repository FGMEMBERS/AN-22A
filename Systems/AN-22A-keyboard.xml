<PropertyList>

  <key n="68">
    <name>D</name>
    <desc>Right door</desc>
    <binding>
      <command>nasal</command>
      <script>doors.crewR.toggle();</script>
    </binding>
  </key>

  <key n="69">
    <name>E</name>
    <desc>Cargo door</desc>
    <binding>
      <command>nasal</command>
      <script>doors.cargo.toggle();</script>
    </binding>
  </key>
  
  <key n="100">
    <name>d</name>
    <desc>Left door</desc>
    <binding>
      <command>nasal</command>
      <script>doors.crewL.toggle();</script>
    </binding>
  </key>
  
   
 <key n="61"> 
    <name>=</name>
    <desc>Paratroopers Jump signal</desc>
    <binding>
      <command>property-toggle</command>
      <property>controls/paratroopers/jump-signal</property>
      <value>true</value>
    </binding>
  </key>
  
  <key n="115">
    <name>s</name>
    <desc>Autostart</desc>
    <binding>
      <command>property-toggle</command>
      <property>controls/autostart</property>
      <value>true</value>
    </binding>
  </key>
 
 <key n="60">
  <name> > </name>
  <desc>Change Pilot Engineer View</desc>
  <binding>
    <command>property-cycle</command>
      <property>sim/current-view/view-number</property>  
      <value>0</value>
      <value>9</value>
  </binding>
</key>

<key n="121">
	<name>y</name>
	<desc>Change Pilot Ramp View</desc>
  <binding>
      <command>property-cycle</command>
      <property>sim/current-view/view-number</property>  
      <value>0</value>
      <value>10</value>
  </binding>
</key>

<key n="127">
	<name>Delete</name>
	<desc>Toggle Reversers</desc>
  <binding>
      <command>property-cycle</command>
      <property>controls/reverser</property>
      <value>true</value>
      <value>false</value>
  </binding>
</key>
  

</PropertyList>



  
